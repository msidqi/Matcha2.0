{
  "name": "next-typescript-tailwind",
  "version": "2.1.0",
  "author": "Alaa Zorkane <https://github.com/alaazorkane>",
  "scripts": {
    "dev": "next",
    "build": "next build",
    "start": "next start",
    "type-check": "tsc",
    "lint:eslint": "eslint 'components/**/*.{js,ts,tsx}'",
    "fix:eslint": "eslint 'components/**/*.{js,ts,tsx}' --fix",
    "lint:prettier": "prettier --check components/**/*.{js,ts,tsx}",
    "fix:prettier": "prettier --write \"components/**/*.{js,ts,tsx}\""
  },
  "dependencies": {
    "@headlessui/react": "^0.2.0",
    "@tailwindcss/custom-forms": "^0.2.1",
    "@zeit/next-css": "^1.0.1",
    "next": "^10.0.3",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-hook-form": "^6.12.2"
  },
  "devDependencies": {
    "@types/node": "^14.11.8",
    "@types/react": "^16.9.46",
    "@types/react-dom": "^16.9.4",
    "@typescript-eslint/eslint-plugin": "^4.4.1",
    "@typescript-eslint/parser": "^4.4.1",
    "eslint": "^7.7.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-react": "^7.20.6",
    "eslint-plugin-react-hooks": "^4.1.0",
    "husky": "^4.2.5",
    "lint-staged": "^10.2.11",
    "postcss-preset-env": "^6.7.0",
    "prettier": "^2.0.5",
    "stylelint": "^13.6.1",
    "stylelint-config-recommended": "^3.0.0",
    "tailwindcss": "^1.8.13",
    "typescript": "4.0.3"
  },
  "lint-staged": {
    "*.{js,ts,jsx,tsx}": [
      "fix:prettier",
      "eslint --fix --max-warnings=0 --cache"
    ]
  },
